    def get_maxval_info(self, input_list):
    
        input_list.sort()
        input_list.reverse()

        result = list(Counter(input_list).values())
        빈도수 = max(result)
        
        if 빈도수 > 2:

            max_index = result.index(max(result))

            # 최대 중복값 산출
            result = list(Counter(input_list).keys())
            value = float(result[max_index])

            return value, 빈도수
        else:
            return None, None

