    def capture_screenshot(self):

        if ResizeRowsToContents:
            self.tableWidget_call.resizeRowsToContents()
        else:
            pass
        self.tableWidget_call.resizeColumnsToContents()

        if ResizeRowsToContents:
            self.tableWidget_put.resizeRowsToContents()
        else:
