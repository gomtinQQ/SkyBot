                txt = "[{0:02d}:{1:02d}:{2:02d}] ▼ Put Strong({3:.2f}/{4:.2f}) ▼\r".format(dt.hour, dt.minute, dt.second, 선물_등락율, DOW_등락율)
                self.textBrowser.append(txt)
            else:
                pass

            flag_heartbeat = True                        
        else:
            pass

