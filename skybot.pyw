        high_low_list = call_low_list + call_high_list + put_low_list + put_high_list
        high_low_list.sort()
        
        moving_list = FILE_HIGH_LOW_LIST + high_low_list
        moving_list.sort()

    def get_value_frequency(self, value):

        frequency = moving_list.count(value)

